{
  "name": "qrway-backend",
  "version": "1.0.0",
  "description": "This directory contains two express servers: * AppServer.js - Combined Node/Express web server w/ Mongo Access * Server.js - Encapsulated Node/Express web server w/ Mongo Access",
  "main": "AppServer.js",
  "dependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.11.1",
    "@types/node": "^18.16.2",
    "assert": "^1.4.1",
    "async": "^3.2.4",
    "body-parser": "^1.20.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "^2.6.3",
    "express": "^4.15.2",
    "express-session": "^1.17.3",
    "mongodb": "^3.1.13",
    "mongoose": "^5.7.5",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "typescript": "^4.6.3",
    "url": "^0.11.0"
  },
  "scripts": {
    "test": "mocha --reporter spec",
    "build": "tsc",
    "build-frontend": "rm -rf ./dist && cd ../frontend && npm run build && cp -r ./dist ../backend/dist && rm -rf ./dist",
    "clean": "rm -rf model/*.js && rm -rf interfaces/*.js && rm -rf *.js",
    "start-dev": "npm run clean && npm run build-frontend && tsc && node AppServer.js",
    "start": "npm run build && node AppServer.js",
    "populate_items": "node ./createDB/createSampleItems.js",
    "populate_menus": "node ./createDB/createSampleMenus.js",
    "populate_restaurants": "node ./createDB/createSampleRestaurants.js",
    "populate_users": "node ./createDB/createSampleUsers.js",
    "populate_db": "npm run populate_items && npm run populate_menus && npm run populate_restaurants && npm run populate_users"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "async": "^3.2.4",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "dotenv": "^16.0.3",
    "mocha": "^10.2.0"
  }
}
